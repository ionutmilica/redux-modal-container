{
  "name": "redux-modal-container",
  "version": "0.1.2",
  "description": "Manage modals in applications that are using react and redux.",
  "author": "Ionut Milica",
  "keywords": [
    "react",
    "redux",
    "modal",
    "container",
    "modals"
  ],
  "homepage": "https://github.com/ionutmilica/redux-modal-container",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionutmilica/redux-modal-container.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib --ignore __tests__",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore __tests__",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/redux-modal-container.js --config config/webpack.dev.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/redux-modal-container.min.js --config config/webpack.prod.js",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test jest --config config/jest.config.js",
    "test:coverage": "npm run test -- --coverage",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test:watch": "npm run test -- --watch --bail",
    "prettier": "prettier --write --semi --single-quote --trailing-comma=all 'src/**/*.js'",
    "clean": "rimraf dist lib es"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "coveralls": "^2.13.1",
    "prettier": "^1.4.4",
    "react": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  }
}
